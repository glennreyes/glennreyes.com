{
  "name": "glennreyes",
  "alias": "glennreyes",
  "builds": [
    {
      "src": "graphql/build/index.js",
      "use": "@now/node-server"
    },
    { "src": "website/package.json", "use": "@now/next" }
  ],
  "env": {
    "GA_TRACKING_ID": "@glennreyes-ga-tracking-id",
    "GITHUB_TOKEN": "@glennreyes-github-token",
    "GOODREADS_KEY": "@glennreyes-goodreads-key",
    "GOODREADS_USER_ID": "@glennreyes-goodreads-user-id"
  },
  "routes": [
    { "src": "/graphql", "dest": "/graphql/build/index.js" },
    { "src": "/(.*)", "dest": "/website/$1" }
  ],
  "version": 2
}
